
public class arithmetic_op {
	public static void  main(String[] args){
		//declare variables
		int value1, value2;
		
		//initialise variables
		value1 = 5;
		value2 = 6;
		
		//arithmetic operation
		float average1 = (value1 + value2)/2;
		System.out.println ("Average 1 = " + average1);
		
		float average2 = (float) (value1 + value2)/2;
		System.out.println ("Average 2 = " + average2);
		
		float average3 = (value1 + value2)/2f;
		System.out.println ( "Average 3 = " + average3);
		
		int average4 = (value1 + value2)/2;
		System.out.println ( "Average 4 = " + average4);
	
}
}
